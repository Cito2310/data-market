{
  "homepage": "./",
  "name": "electron-react-template",
  "version": "1.1.2",
  "author": "",
  "description": "",
  "keywords": [
    "electron",
    "react",
    "template",
    "typescript",
    "basic"
  ],
  "main": "build/electron/main.js",
  "private": true,
  "dependencies": {
    "@types/node": "^18.19.44",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.7.4",
    "date-fns": "^3.6.0",
    "react": "^18.2.0",
    "react-charts": "^3.0.0-beta.57",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.52.2",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "dev": "concurrently \"cross-env BROWSER=none react-scripts start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "build": "react-scripts build && tsc -p electron && electron-builder",
    "update-patch": "npm version --force --no-git-tag-version patch",
    "update-minor": "npm version --force --no-git-tag-version minor",
    "update-major": "npm version --force --no-git-tag-version major"
  },
  "build": {
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^22.3.27",
    "electron-builder": "^23.6.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
